{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "Kundenanliegen (OpenAi)",
  "id": "7073c7df-996e-4ad2-8a74-623c2d4aeab6",
  "appliesTo": [
    "bpmn:Task"
  ],
  "icon": {
    "contents": "data:image/svg+xml;base64,"
  },
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "label": "Open AI",
      "id": "openai"
    },
    {
      "label": "Output",
      "id": "output"
    }
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "de.viadee.bpm.zeebe.connector:openai:1",
      "binding": {
        "type": "zeebe:taskDefinition:type"
      }
    },
    {
      "label": "Input",
      "description": "Name of the variable to use as input",
      "group": "openai",
      "type": "Text",
      "feel": "required",
      "binding": {
        "type": "zeebe:input",
        "name": "openai_input"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Operation",
      "description": "Operation to perform",
      "group": "openai",
      "type": "Dropdown",
      "id": "open_ai_type",
      "choices": [
        {
          "name": "Sentiment analysis",
          "value": "sentiment_analysis"
        },
        {
          "name": "Customer concern recognition",
          "value": "customer_concerns"
        }
      ],
      "binding": {
        "name": "openai_function",
        "type": "zeebe:input"
      }
    },
    {
      "label": "Result Variable",
      "description": "Name of variable to store the response in",
      "group": "output",
      "type": "String",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultVariable"
      }
    },
    {
      "label": "Result Expression",
      "description": "Expression to map the response into process variables",
      "group": "output",
      "type": "Text",
      "feel": "required",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      }
    }
  ]
}
